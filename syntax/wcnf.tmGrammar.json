{
  "scopeName": "source.dimacs.wcnf",
  "patterns": [
    {
      "match": "^c.*$",
      "name": "source.dimacs comment"
    },
    {
      "match": "0$",
      "name": "source.dimacs zero"
    },
    {
      "match": "^(h|\\b(1|2|3|4|5|6|7|8|9)\\d*)\\s",
      "name": "source.dimacs weight"
    },
    {
      "match": "\\b(1|2|3|4|5|6|7|8|9)\\d*\\b",
      "name": "source.dimacs literal"
    },
    {
      "match": "-",
      "name": "source.dimacs minus"
    }
  ],
  "repository": {}
}